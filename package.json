{
  "name": "starter",
  "version": "1.0.0",
  "description": "A quick up and running node ts project",
  "main": "index.ts",
  "scripts": {
    "test": "jest",
    "bundle:watch": "webpack --watch",
    "build": "webpack",
    "server": "nodemon --exec node build/server.js",
    "format": "prettier --write \"**/*.+(ts|json|js|)\"",
    "start": "concurrently \"yarn run bundle:watch\" \"yarn run server\"",
    "production": "webpack && node build/server.js"
  },
  "author": "ctobolski",
  "license": "MIT",
  "devDependencies": {
    "@types/axios": "0.14.0",
    "@types/express": "^4.16.1",
    "@types/jest": "^24.0.11",
    "@types/jsonwebtoken": "8.3.2",
    "@types/node": "^11.11.3",
    "@types/pg": "7.4.14",
    "@types/qs": "6.5.3",
    "concurrently": "^4.1.0",
    "jest": "^24.5.0",
    "nodemon": "^1.18.10",
    "prettier": "^1.16.4",
    "ts-jest": "^24.0.0",
    "ts-loader": "^5.3.3",
    "typescript": "^3.3.3333",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "express-jwt": "5.3.1",
    "jsonwebtoken": "8.5.1",
    "jwks-rsa": "1.4.0",
    "pg": "7.10.0",
    "qs": "6.7.0"
  }
}
